<?xml version="1.0" encoding="UTF-8"?>
<process
    name="ReceiveReply-FromParts"
    targetNamespace="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/bpel/receiveReplyFromParts"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:ti="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/wsdl/testinterface"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <import namespace="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/wsdl/testinterface" location="./TestInterface-MessageWithoutParts.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="ReplyData" messageType="ti:executeProcessSyncResponse"/>
        <variable name="TempInt" type="xs:int" />
    </variables>
    <sequence>
    	<!-- receive and reply operation does not contain any <message><part> definitions -->
        <receive name="InitialReceive" partnerLink="MyRoleLink" operation="startProcessSyncWithEmptyMessage" portType="ti:TestInterfacePortType" createInstance="yes">
            <!-- ...thus <fromParts> is not allowed -->
            <fromParts>
                <fromPart part="inputPart" toVariable="TempInt" />
            </fromParts>
        </receive>
        <assign>
            <copy>
                <from variable="TempInt"/>
                <to variable="ReplyData" part="outputPart"/>
            </copy>
        </assign>
        <reply name="CorrelatedReply" partnerLink="MyRoleLink" operation="startProcessSyncWithEmptyMessage" portType="ti:TestInterfacePortType" variable="ReplyData" />
    </sequence>
</process>